{
  "name": "workspace",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "release": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": true,
        "trackDeps": true,
      }
    },
    "prerelease": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": true,
        "preid": "next",
        "trackDeps": true,
      }
    }
  }
}
